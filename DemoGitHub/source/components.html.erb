  <div id="mainnav" class="Component">
    <h2 class="Component-head">MainNav</h2>
    <div class="MainNav" data-controller="mainNav" style="padding: 0; width: 100%; float: none;">
      <%= partial 'partials/main_nav_primary' %>
    </div>
  </div>
  <div id="mainnav--stretch" class="Component">
    <h2 class="Component-head">MainNav--stretch</h2>
    <div class="MainNav MainNav--stretch" data-controller="mainNav" style="padding: 0; width: 100%; float: none;">
      <%= partial 'partials/main_nav_primary' %>
    </div>
  </div>

  <div id="sidebar" class="Component">
    <h2 class="Component-head">Sidebar</h2>
    <%= partial 'partials/components/sidebar' %>
  </div>

  <div id="content-block" class="Component">
    <h2 class="Component-head">Content block</h2>
    <%= partial 'partials/components/content_block', locals: { color: 'blue', icon: 'bubbles' } %>
    <%= partial 'partials/components/content_block', locals: { color: 'green', icon: 'hotdog' } %>
    <%= partial 'partials/components/content_block', locals: { color: 'orange', icon: nil } %>
  </div>

  <div id="action-block" class="Component">
    <h2 class="Component-head">Action block</h2>
    <%= partial 'partials/components/action_block', locals: { variant: 'full', color: 'orange' } %>
    <br>
    <%= partial 'partials/components/action_block', locals: { variant: 'medium', color: 'green' } %>
  </div>

  <div id="article-list" class="Component">
    <h2 class="Component-head">Article list</h2>
    <%= partial 'partials/components/article_list', locals: { split: false, item_count: 2, has_image: true } %>
    <%= partial 'partials/components/article_list', locals: { split: true, item_count: 4, has_image: true } %>
    <%= partial 'partials/components/article_list', locals: { split: false, item_count: 4, has_image: false } %>
  </div>

  <div id="action-bar" class="Component">
    <h2 class="Component-head">Action bar</h2>
    <%= partial 'partials/components/action_bar', locals: { items: 3 } %>
    <%= partial 'partials/components/action_bar', locals: { items: 2 } %>
    <%= partial 'partials/components/action_bar', locals: { items: 1 } %>
  </div>

  <div id="fuel-types" class="Component">
    <h2 class="Component-head">Fuel types</h2>
    <%= partial 'partials/components/fuel_types' %>
  </div>

  <div id="buttons" class="Component">
