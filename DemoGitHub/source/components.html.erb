    <button class="Button Button--full">Hej hej</button>
    <br><br>
    <button class="Button Button--green Button--full">Hej hej</button>
  </div>

  <div id="checklist" class="Component">
    <h2 class="Component-head">Checklist</h2>
    <%= partial 'partials/components/checklist' %>
  </div>

  <div id="video" class="Component">
    <h2 class="Component-head">Video</h2>
    <%= partial 'partials/components/video' %>
  </div>

  <div id="share-actions" class="Component">
    <h2 class="Component-head">Share actions</h2>
    <%= partial 'partials/components/share_actions', locals: { on_color: false } %>
    <%= partial 'partials/components/share_actions', locals: { on_color: true } %>
  </div>

  <div id="featured-block" class="Component">
    <h2 class="Component-head">Featured block</h2>

    <div class="ContentBlock ContentBlock--orange Article">
      <header class="ContentBlock-header">
        <h2 class="ContentBlock-headline">Featured block</h2>
      </header>
      <%= partial 'partials/components/featured_block', locals: { headline: 'Anslut kort &amp; spara pengar' } %>
    </div>
    <br><br>
    <div class="ContentBlock ContentBlock--green Article">
      <header class="ContentBlock-header">
        <h2 class="ContentBlock-headline">Featured block</h2>
      </header>
      <%= partial 'partials/components/featured_block', locals: { headline: 'Anslut kort &amp; spara pengar' } %>
    </div>
    <br><br>
    <div class="ContentBlock ContentBlock--blue Article">
      <header class="ContentBlock-header">
        <h2 class="ContentBlock-headline">Featured block</h2>
      </header>
      <%= partial 'partials/components/featured_block', locals: { headline: 'Anslut kort &amp; spara pengar' } %>
    </div>
    <br><br>
    <div class="ContentBlock ContentBlock--facebookBlue Article">
      <header class="ContentBlock-header">
        <h2 class="ContentBlock-headline">Featured block</h2>
      </header>
      <%= partial 'partials/components/featured_block', locals: { headline: 'Anslut kort &amp; spara pengar' } %>
    </div>
  </div>

  <div id="station-finder-mini" class="Component">
    <h2 class="Component-head">Station finder mini</h2>
    <%= partial 'partials/components/station_finder_mini', locals: { show_favorites: false, has_last_visited: true } %>
    <%= partial 'partials/components/station_finder_mini', locals: { show_favorites: true, has_favorites: true, has_last_visited: true } %>
    <%= partial 'partials/components/station_finder_mini', locals: { show_favorites: true, has_favorites: false, has_last_visited: false } %>
  </div>

  <div id="station-list" class="Component">
