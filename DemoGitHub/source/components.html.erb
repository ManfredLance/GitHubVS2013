  </div>

  <div id="slideshow" class="Component">
    <h2 class="Component-head">Slideshow</h2>
    <%= partial 'partials/components/slideshow' %>
  </div>

  <div id="notices" class="Component">
    <h2 class="Component-head">Notices</h2>
    <%= partial 'partials/components/notice' %>
    <%= partial 'partials/components/notice', locals: { type: 'alert' } %>
  </div>

  <div id="item-list" class="Component">
    <h2 class="Component-head">Item list</h2>
    <%= partial 'partials/components/item_list', locals: { price: false, square: false } %>
    <%= partial 'partials/components/item_list', locals: { price: true, square: false } %>
    <%= partial 'partials/components/item_list', locals: { price: true, square: true } %>
  </div>

  <div id="index-navigation" class="Component">
    <h2 class="Component-head">Index navigation</h2>
    <%= partial 'partials/components/index_nav' %>
  </div>

  <div id="section-list" class="Component">
    <h2 class="Component-head">Section list</h2>
    <%= partial 'partials/components/section_list' %>
  </div>

  <div id="section-list" class="Component">
    <h2 class="Component-head">Table</h2>
    <%= partial 'partials/components/table' %>
  </div>

  <div id="full" class="Component">
    <h2 class="Component-head">Full</h2>
    <%= partial 'partials/components/full', locals: { aligncenter: true } %>
    <%= partial 'partials/components/full', locals: { aligncenter: false } %>
  </div>

</div>

<% content_for :footer_script do %>
  <%= javascript_include_tag "//maps.googleapis.com/maps/api/js?key=#{google_maps_api_key}&sensor=false&libraries=places", type: nil %>
<% end %>
